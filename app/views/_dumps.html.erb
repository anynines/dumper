<div class="row dumper">
  <% unless @dumps.blank? %>
      <table class="table table-striped table-hover">
        <tr>
          <th>Filename</th>
          <th>Filesize</th>
          <th>&nbsp;</th>
        </tr>

        <%= render(partial: '/dump', collection: @dumps) %>
      </table>

  <% else %>
      <div class="alert alert-info" role="alert">
        <p>No dumps found.</p>
      </div>
  <% end %>

</div>